<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="connect-src">
            <values>
                <value id="ages_app" type="host">*.ages.app</value>
                <value id="ages_app_wss" type="host">wss://*.ages.app</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="ages_app" type="host">*.ages.app</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="ages_app" type="host">*.ages.app</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
